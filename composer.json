{
    "name": "mgid/pagination-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Pagination Bundle",
    "keywords": ["symfony", "bundle", "pagination", "doctrine", "orm", "odm"],
    "homepage": "https://github.com/mgid/pagination-bundle",
    "license": "MIT",
    "authors": [{"name": "Dmitry Khaperets", "email": "dmitry.khaperets@mgid.com"}],
    "autoload": {
        "psr-4": {
            "Mgid\\PaginationBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mgid\\PaginationBundle\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.4",
        "mgid/pagination": "^1.0",
        "symfony/framework-bundle": "^5.1",
        "symfony/yaml": "^5.1"
    },
    "require-dev": {
        "doctrine/orm": "^2.7",
        "phpstan/phpstan": "^0.12",
        "phpunit/php-code-coverage": "^9.2",
        "phpunit/phpunit": "^9.4"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
